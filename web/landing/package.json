{
  "name": "scriberr-landing",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "sync:spec": "cp -f ../../api-docs/swagger.json public/api/swagger.json",
    "sync:undoc": "node ./scripts/gen-endpoints.mjs",
    "dev": "npm run sync:spec && npm run sync:undoc && vite",
