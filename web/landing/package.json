    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "@vitejs/plugin-react": "^4.3.1",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.4.0",
    "vite": "^5.3.0"
  }
}
